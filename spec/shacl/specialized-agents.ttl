# =============================================================================
# SHACL Parameter Shapes for Specialized Agent Types
# Covers: Reviewer, Deployer, Tester
# =============================================================================

# =============================================================================
# Reviewer Agent Parameters
# =============================================================================

# -----------------------------------------------------------------------------
# Review Parameters
# -----------------------------------------------------------------------------

params:ReviewParamsShape a sh:NodeShape ;
    sh:targetClass params:ReviewParams ;
    rdfs:label "Review Parameters Shape"@en ;
    rdfs:comment "Parameters for conducting a code/artifact review."@en ;

    sh:property [
        sh:path params:targetRef ;
        sh:name "targetRef" ;
        sh:description "Reference to the artifact being reviewed (file, PR, commit)" ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:string ;
        sh:minLength 1
    ] ;

    sh:property [
        sh:path params:reviewType ;
        sh:name "reviewType" ;
        sh:description "Type of review to conduct" ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:in ( "code" "security" "architecture" "documentation" "performance" )
    ] ;

    sh:property [
        sh:path params:criteria ;
        sh:name "criteria" ;
        sh:description "Specific criteria or checklist items to evaluate" ;
        sh:datatype xsd:string
    ] ;

    sh:property [
        sh:path params:depth ;
        sh:name "depth" ;
        sh:description "Depth of review" ;
        sh:maxCount 1 ;
        sh:in ( "quick" "standard" "thorough" )
    ] ;

    sh:property [
        sh:path params:focusAreas ;
        sh:name "focusAreas" ;
        sh:description "Specific areas to focus on" ;
        sh:datatype xsd:string
    ] .

# -----------------------------------------------------------------------------
# Approve (Reviewer) Parameters
# -----------------------------------------------------------------------------

params:ReviewApproveParamsShape a sh:NodeShape ;
    sh:targetClass params:ReviewApproveParams ;
    rdfs:label "Review Approve Parameters Shape"@en ;
    rdfs:comment "Parameters for approving a review item."@en ;

    sh:property [
        sh:path params:reviewRef ;
        sh:name "reviewRef" ;
        sh:description "Reference to the review being approved" ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:string
    ] ;

    sh:property [
        sh:path params:approvalType ;
        sh:name "approvalType" ;
        sh:description "Type of approval" ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:in ( "approved" "approved_with_suggestions" "conditional" )
    ] ;

    sh:property [
        sh:path params:rationale ;
        sh:name "rationale" ;
        sh:description "Reason for approval" ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
