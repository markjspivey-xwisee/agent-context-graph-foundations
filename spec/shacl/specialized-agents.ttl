# -----------------------------------------------------------------------------
# FlagSecurity Parameters
# -----------------------------------------------------------------------------

params:FlagSecurityParamsShape a sh:NodeShape ;
    sh:targetClass params:FlagSecurityParams ;
    rdfs:label "FlagSecurity Parameters Shape"@en ;
    rdfs:comment "Parameters for flagging a security issue."@en ;

    sh:property [
        sh:path params:issueType ;
        sh:name "issueType" ;
        sh:description "Type of security issue" ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:in ( "vulnerability" "exposure" "weak_crypto" "injection" "auth_bypass" "data_leak" "other" )
    ] ;

    sh:property [
        sh:path params:severity ;
        sh:name "severity" ;
        sh:description "Severity level (CVSS-aligned)" ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:in ( "low" "medium" "high" "critical" )
    ] ;

    sh:property [
        sh:path params:cweId ;
        sh:name "cweId" ;
        sh:description "CWE identifier if applicable" ;
        sh:maxCount 1 ;
        sh:datatype xsd:string ;
        sh:pattern "^CWE-[0-9]+$"
    ] ;

    sh:property [
        sh:path params:location ;
        sh:name "location" ;
        sh:description "Location of the security issue" ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:string
    ] ;

    sh:property [
        sh:path params:description ;
        sh:name "description" ;
        sh:description "Detailed description of the issue" ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:string
    ] ;

    sh:property [
        sh:path params:remediation ;
        sh:name "remediation" ;
        sh:description "Suggested remediation steps" ;
        sh:minCount 1 ;
        sh:datatype xsd:string
    ] ;

    sh:property [
        sh:path params:blockMerge ;
        sh:name "blockMerge" ;
        sh:description "Whether this should block merge/deployment" ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:boolean
    ] .

